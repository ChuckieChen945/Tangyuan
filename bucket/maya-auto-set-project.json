    "hash": "0743773d2595a82cdd7293b83be4b0813f65fcee2983401a29be2a5b62e9d08d",
    "extract_dir": ["auto_set_project-main/plug-ins"],
    "depends": ["ChuckieChen945_Tangyuan/maya-np"],
    "installer": {
        "script": [
            "New-Item -Path \"$env:USERPROFILE\\Documents\\maya\\plug-ins\\\" -ItemType Directory -Force",
            "New-Item -ItemType SymbolicLink -Path \"$env:USERPROFILE\\Documents\\maya\\plug-ins\\autoSetProject.py\" -Target \"$dir\\autoSetProject.py\""
        ]
    },
    "uninstaller": {
        "script": [
            "Remove-Item -Path \"$env:USERPROFILE\\Documents\\maya\\plug-ins\\autoSetProject.py\" -Recurse -Force"
        ]
    },
    "checkver": {
        "url": "https://github.com/masonSmigel/auto_set_project/commits/main.atom",
        "regex": "(?s)>(\\d+)-(\\d+)-(\\d+)T.*?/(?<sha>(?<short>[0-9a-f]{7})[0-9a-f]{33})",
        "replace": "${1}.${2}.${3}.${short}"
    },
    "autoupdate": {
        "url": [
            "https://github.com/masonSmigel/auto_set_project/archive/refs/heads/main.zip"
        ]
    }
}
